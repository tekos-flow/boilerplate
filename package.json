{
    "name": "Tekos boilerplate Template ",
    "description": "This is the default boilerplate tekos template.",
    "version": "0.0.1",
    "tekos-bot-required": true,
    "dependencies": {
        "node-red-contrib-tekosbot": "0.0.35",
        "node-red-node-email": "1.7.3",
        "node-red-contrib-process-env": "0.2.5",
        "node-red-contrib-dialogflowv2-api": "1.0.4",
        "node-red-contrib-audio-convert": "0.0.7",
        "node-red-node-base64": "0.2.1",
        "node-red-node-watson": "0.9.2"
    },
    "node-red": {
        "settings": {
            "flowFile": "flow.json",
            "credentialsFile": "flows_cred.json"
        }
    },
    "website": "https://tekos.co",
    "repository": "https://github.com/tekos-flow/boilerplate",
    "logo": "https://tekos.co/wp-content/uploads/2019/06/tekos-logo.png",
    "keywords": [
        "bot",
        "boilerplate",
        "Tekos"
    ],
    "env": {
        "SECRET_TOKEN": {
            "description": "A secret key for segment",
            "generator": "secret"
        },
        "WEB_CONCURRENCY": {
            "description": "The number of processes to run.",
            "value": "5"
        },
        "STRIPE_KEY": {
            "description": "Stripe key.",
            "required": "false"
        }
    }
}